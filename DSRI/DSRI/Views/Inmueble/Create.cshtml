@model Datos.DSRIFINMUEBLE

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Right side column. Contains the navbar and content of the page -->
<aside class="right-side">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Nuevo Inmueble
            <small>Creación</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="glyphicon glyphicon-cog"></i>Configuración del Sistema</a></li>
            <li><a href="#">Administrar Inmueble</a></li>
            <li><a href="#">Nuevo Inmueble</a></li>
        </ol>
    </section>

    <section class="content">
        <div class="row">
            <!-- right column -->
            <div class="col-md-6">
                <!-- general form elements disabled -->
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Datos de Inmueble</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()

                            <div class="form-horizontal">
                                <hr />
                                @Html.ValidationSummary(true)

                                <div class="form-group">

                                    @Html.LabelFor(model => model.COD_INMUEBLE, new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        <input class="form-control" id="COD_INMUEBLE" name="COD_INMUEBLE" type="text" value="" />
                                        @Html.ValidationMessageFor(model => model.COD_INMUEBLE)
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.COD_CLASIFICACION, "Clasificación", new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.DropDownList("COD_CLASIFICACION", null, null, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.COD_CLASIFICACION)
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.NOM_INMUEBLE, new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        <input class="form-control" id="NOM_INMUEBLE" name="NOM_INMUEBLE" type="text" value="" />
                                        @Html.ValidationMessageFor(model => model.NOM_INMUEBLE)
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.CAN_CAPACIDAD, new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        <input class="form-control" id="CAN_CAPACIDAD" name="CAN_CAPACIDAD" type="number" value="" />
                                        @Html.ValidationMessageFor(model => model.CAN_CAPACIDAD)
                                    </div>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.COS_INMUEBLE, new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        <input class="form-control" id="COS_INMUEBLE" name="COS_INMUEBLE" type="number" value="" />
                                        @Html.ValidationMessageFor(model => model.COS_INMUEBLE)
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="col-md-offset-2 col-md-10">
                                        <input type="submit" value="Agregar" class="btn btn-primary" />
                                        |
                                        @Html.ActionLink("Regresar al contenido principal", "Index", null, new { @class = "btn btn-default" })
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>
            </div>
        </div>
    </section>

</aside>

@Scripts.Render("~/bundles/endpage")
<script type="text/javascript">
    $('select#COD_CLASIFICACION').prop('selectedIndex', 0);
</script>
