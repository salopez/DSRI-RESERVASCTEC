@model Datos.DSRIFINMUEBLE

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Right side column. Contains the navbar and content of the page -->
<aside class="right-side">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Eliminar Inmueble
                        <small>Eliminar</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="glyphicon glyphicon-cog"></i>Configuración del Sistema</a></li>
            <li><a href="#">Administrar Inmubles</a></li>
            <li><a href="#">Eliminar Inmuble</a></li>
        </ol>
    </section>

    <section class="content">
        <div class="row">
            <!-- right column -->
            <div class="col-md-6">
                <!-- general form elements disabled -->
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Datos de Inmuble</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">

                        <h2>@Html.DisplayFor(model => model.NOM_INMUEBLE) </h2>

                        <h5>¿Está seguro que desea borrar el Inmuble?</h5>
                        <div>
                            <hr />
                            <dl class="dl-horizontal">

                                <dt>
                                    @Html.DisplayNameFor(model => model.CAN_CAPACIDAD)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.CAN_CAPACIDAD)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.DSRIFCLASIFINMUEBLE.NOM_CLASIFINMUEBLE)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.DSRIFCLASIFINMUEBLE.NOM_CLASIFINMUEBLE)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.COS_INMUEBLE)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.COS_INMUEBLE)
                                </dd>


                            </dl>

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Eliminar" class="btn btn-danger" />
                                    |
            @Html.ActionLink("Regresar al contenido principal", "Index", null, new { @class = "btn btn-default" })
                                </div>
                            }
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>
</aside>
@Scripts.Render("~/bundles/endpage")